<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 建立类与表的映射 -->
    <class name="model.DProject" table="dproject">
        <!-- 建立类中的属性与表中的主键对应 -->
        <id name="ID" column="ID" >
            <generator class="assigned"/>
        </id>

        <!-- 建立类中的普通的属性和表的字段的对应 -->
        <property name="teacherId"/>
        <property name="Unit"/>
        <property name="ResultName"/>
        <property name="ProjectUnit"/>
        <property name="ETime"/>
        <property name="IfApply"/>
        <property name="StudyType"/>
        <property name="ApplyObject"/>
        <property name="OneStudy"/>
        <property name="ProjectSource"/>
        <property name="School"/>
        <property name="Evaluate"/>
        <property name="Money1"/>
        <property name="Money2"/>

    </class>
</hibernate-mapping>