<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 建立类与表的映射 -->
    <class name="model.Art" table="art">
        <!-- 建立类中的属性与表中的主键对应 -->
        <id name="ResultName" column="ResultName" >
            <generator class="assigned"/>
        </id>
        <!-- 建立类中的普通的属性和表的字段的对应 -->
        <property name="teacherId" column="teacherId" />
        <property name="Unit" column="Unit"/>
        <property name="STime" column="STime"/>
        <property name="PublicationName" column=" PublicationName"/>
        <property name="IfAward" column="IfAward"/>
        <property name="Kind" column="Kind"/>
        <property name="School" column="School"/>
    </class>
</hibernate-mapping>