<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 建立类与表的映射 -->
    <class name="model.PAward" table="part">
        <!-- 建立类中的属性与表中的主键对应 -->
        <id name="teacherId" column="teacherId" >
            <generator class="assigned"/>
        </id>

        <!-- 建立类中的普通的属性和表的字段的对应 -->
        <property name="Name"  />
        <property name="ID" />
        <property name="Sex"/>
        <property name="Contribution" />
        <property name="Kind" />
        <property name="Education"/>
        <property name="Job" />
        <property name="Unit" />
    </class>
</hibernate-mapping>