<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <!-- 建立类与表的映射 -->
    <class name="model.Team" table="team">
        <!-- 建立类中的属性与表中的主键对应 -->
        <id name="teacherId" column="teacherId">
            <generator class="assigned"/>
        </id>

        <!-- 建立类中的普通的属性和表的字段的对应 -->
        <property name="name"/>
        <property name="kind"/>
        <property name="allNumber"/>
        <property name="numb1"/>
        <property name="numb2"/>
        <property name="numb3"/>
        <property name="numb4"/>
        <property name="numb5"/>
        <property name="numb6"/>
        <property name="Atime1"/>
        <property name="Status1"/>
    </class>
</hibernate-mapping>